        "esbuild": "^0.19.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.26",
        "postcss-import": "^15.1.0",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "tailwindcss": "^3.3.3"
    },
    "dependencies": {
        "cropperjs": "1.5.13",
        "filepond": "4.31.1",
        "filepond-plugin-file-metadata": "^1.0.8",
